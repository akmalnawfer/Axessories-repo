<body>

    <!-- Page Wrapper -->
    <div id="wrap" class="layout-8">
        <!-- Top bar -->
        <div class="top-bar">
            <div class="container">
                <div class="right-sec">
                    @Html.ActionLink("Register", "Register", "Security")
                </div>
                <div class="right-sec">
                    @Html.ActionLink("Login", "Login", "Security")
                </div>
            </div>
        </div>

        <!-- Header -->
        <header>
            <div class="container">
                <div class="logo"> <a href="index.html"><img src="~/Content/images/logo.png" alt=""></a> </div>
                @Html.Partial("_Search")
