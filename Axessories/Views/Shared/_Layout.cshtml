    <script type="text/javascript">
        /* ==========================================================================
            countdown timer
        ========================================================================== */
        (function ($) {
            "use strict";
            $('.countdown').downCount({
                date: '12/12/2018 12:00:00' // M/D/Y
            });
        })(jQuery);
    </script>
